@model MvcFirstCodeEntity.Models.Classes.YETENEKLER

@{
    ViewBag.Title = "YetenekGuncelle";
    Layout = "~/Views/Shared/_LayoutNavBar.cshtml";
}

<form class="form-group" method="post">
    <div style="margin-bottom:30px">
        <label>Yetenek Açıklama</label>
        @Html.TextBoxFor(x => x.ACIKLAMA, new { @class = "form-control" })
    </div>
    <div style="margin-bottom:20px">
        <label>Yetenek Değer</label>
        @Html.TextBoxFor(x => x.DEGER, new { @class = "form-control" })
    </div>
    <div>
        <button class="btn btn-warning">Guncelle</button>
    </div>
</form>